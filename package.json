  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "develop": "npm-run-all --parallel develop:*",
    "build": "npm-run-all --parallel build:*",
    "serve": "npm-run-all --parallel serve:*",
    "surge": "npm-run-all --parallel surge:*",
    "clean": "npm-run-all --parallel clean:*",
    "GATSBY": "",
    "start:gatsby": "npm run develop:gatsby",
    "develop:gatsby": "cross-env PROJECT_ENV=gatsby GATSBY_DB_NODES=loki GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "build:gatsby": "cross-env PROJECT_ENV=gatsby GATSBY_DB_NODES=loki gatsby build",
    "serve:gatsby": "gatsby serve",
    "surge:gatsby": "npx surge --domain $(node -p \"require('./package.json').name\").surge.sh --project ./public",
    "clean:gatsby": "gatsby clean",
    "STORYBOOK": "",
    "start:storybook": "npm run develop:storybook",
    "develop:storybook": "cross-env PROJECT_ENV=storybook start-storybook -p 6006 -s ./static --ci --quiet",
    "build:storybook": "cross-env PROJECT_ENV=storybook build-storybook -s ./static",
    "serve:storybook": "npx serve ./storybook-static",
    "surge:storybook": "npx surge --domain $(node -p \"require('./package.json').name\")-storybook.surge.sh --project ./storybook-static",
    "clean:storybook": "rimraf ./storybook-static",
    "TSDX": "",
    "start:tsdx": "npm run develop:tsdx",
    "develop:tsdx": "npm-run-all develop:tsdx:pre --parallel develop:tsdx:index develop:tsdx:post",
    "develop:tsdx:index": "cross-env PROJECT_ENV=tsdx tsdx watch --verbose --entry ./src/component-library/index.tsx",
    "develop:tsdx:pre": "cross-env PROJECT_ENV=tsdx NODE_ENV=development node ./scripts.tsdx.js",
    "develop:tsdx:post": "cross-env PROJECT_ENV=tsdx NODE_ENV=development WATCH=true node ./scripts.tsdx.js",
    "build:tsdx": "npm-run-all --parallel build:tsdx:*",
    "build:tsdx:index": "cross-env PROJECT_ENV=tsdx NODE_ENV=production tsdx build --entry ./src/component-library/index.tsx",
    "build:tsdx:pre": "cross-env PROJECT_ENV=tsdx NODE_ENV=production node ./scripts.tsdx.js",
    "serve:tsdx": "",
    "surge:tsdx": "",
    "clean:tsdx": "rimraf dist",
    "TEST": "",
    "start:test": "npm run develop:test",
    "develop:test": "jest --watch",
    "build:test": "jest --coverage",
    "debug:test": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "start-cypress": "npm run develop:cypress",
    "develop-cypress": "start-server-and-test start:gatsby http://localhost:8000 'cypress open --config baseUrl=http://localhost:8000'",
    "build-cypress": "start-server-and-test 'npm-run-all build:gatsby serve:gatsby' http://localhost:9000 'cypress run --config baseUrl=http://localhost:9000'",
    "MAINTENANCE": "",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "test:gatsby": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "ts-typie": "npx ts-typie",
    "ncu": "npx ncu"
  }
